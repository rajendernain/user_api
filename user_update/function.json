{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ],
      "route": "user/update"
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"

    },    
    {
      "type": "cosmosDB",
      "direction": "out",
      "name": "userDbObj",
      "databaseName": "cellchk",
      "collectionName": "user",
      "createIfNotExists": false,
      "connectionStringSetting": "CosmosDbConnectionString"
    }
  ]
}
